
.wxapp-canvas__wrap {
    position: relative;
}

.wxapp-canvas {
    width: 100%;
    height: 100%;
}

.wxapp-canvas-view {
    position: absolute;
    display: block;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: -1;
}

.wxapp-aid-canvas {
    position: fixed;
    left: 200vw;
    top: 200vh;
}